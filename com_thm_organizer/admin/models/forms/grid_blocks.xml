<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset name="periods">
        <field name="startTime" type="text" class="inputbox"
               label="COM_THM_ORGANIZER_START_TIME" description="COM_THM_ORGANIZER_START_TIME_DESC"/>
        <field name="endTime" type="text" class="inputbox"
               label="COM_THM_ORGANIZER_END_TIME" description="COM_THM_ORGANIZER_END_TIME_DESC"/>
        <field name="label_de" type="text" class="inputbox"
               label="COM_THM_ORGANIZER_NAME_DE" description="COM_THM_ORGANIZER_NAME_DE_DESC"/>
        <field name="label_en" type="text" class="inputbox"
               label="COM_THM_ORGANIZER_NAME_EN" description="COM_THM_ORGANIZER_NAME_EN_DESC"/>
        <field name="type" type="list" class="inputbox" default="" label="COM_THM_ORGANIZER_TYPE">
            <option value="">COM_THM_ORGANIZER_DEFAULT</option>
            <option value="break">COM_THM_ORGANIZER_BREAK</option>
        </field>
    </fieldset>
</form>
