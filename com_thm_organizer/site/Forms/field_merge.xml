<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Organizer\Fields">
    <fieldset name="details">
        <field name="field_de" type="MergeValues" class="inputbox" label="NAME_DE" />
        <field name="field_en" type="MergeValues" class="inputbox" label="NAME_EN" />
        <field name="untisID" type="MergeValues" class="inputbox" label="UNTISID" />
        <field name="colorID" type="MergeAssociations" class="inputbox" label="COLOR"
               textcolumn="name" localized="true" from="fields AS f" innerJoins="colors AS c ON c.id = f.colorID"/>
        <field name="ids" type="MergeIDs" hidden="true"/>
    </fieldset>
</form>
