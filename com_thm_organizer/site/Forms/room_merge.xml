<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Organizer\Fields">
    <fieldset name="details">
        <field name="untisID" type="MergeValues" class="inputbox" label="UNTISID"/>
        <field name="name" type="MergeValues" class="inputbox" label="NAME"/>
        <field name="buildingID" type="MergeAssociations" class="inputbox" label="BUILDING"
               textcolumn="b.name" from="rooms AS rm" innerJoins="buildings AS b ON b.id = rm.buildingID"/>
        <field name="typeID" type="MergeAssociations" class="inputbox" label="TYPE"
               textcolumn="name" localized="true" from="rooms AS rm" innerJoins="room_types AS rt ON rt.id = rm.typeID"/>
        <field name="capacity" type="MergeValues" class="inputbox" label="CAPACITY"/>
        <field name="ids" type="MergeIDs" hidden="true"/>
    </fieldset>
</form>
