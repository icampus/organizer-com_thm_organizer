<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Organizer\Fields">
    <fieldset name="details">
        <field name="untisID" type="MergeValues" class="inputbox" label="UNTISID" />
        <field name="name" type="MergeValues" class="inputbox" label="NAME" />
        <field name="programID" type="MergeAssociations" class="inputbox" label="PROGRAM"
               textcolumn="prg.name,prg.version,dgr.abbreviation" localized="true" glue=", " from="categories AS cat"
               innerJoins="programs AS prg ON prg.id = cat.programID,degrees AS dgr ON dgr.id = prg.degreeID"/>
        <field name="departmentID" type="MergeDepartments" class="inputbox" label="DEPARTMENTS" resource="category" multiple="multiple"/>
        <field name="ids" type="MergeIDs" hidden="true"/>
    </fieldset>
</form>
