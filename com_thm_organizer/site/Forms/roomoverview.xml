<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Organizer\Fields">
    <fieldset name="fields">
        <field name="template" type="Templates" label="DISPLAY_RANGE" default="1" translateLabel="false">
            <option value="1">SINGLE_DAY</option>
            <option value="2">WEEK</option>
        </field>
        <field name="date" type="Date" label="DATE"/>
        <field name="campusID" type="Campuses" label="CAMPUS" translateLabel="false">
            <option value="">ALL_CAMPUSES</option>
        </field>
        <field name="buildingID" type="Buildings" label="BUILDING" translateLabel="false">
            <option value="">ALL_BUILDINGS</option>
        </field>
        <field name="roomtypeIDs" type="Roomtypes" label="ROOMTYPES" multiple="true" translateLabel="false">
            <option value="">ALL_ROOMTYPES</option>
        </field>
        <field name="roomIDs" type="Rooms" label="ROOMS" multiple="true" translateLabel="false">
            <option value="">ALL_ROOMS</option>
        </field>
    </fieldset>
</form>
